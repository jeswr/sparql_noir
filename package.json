    "@zk-kit/eddsa-poseidon": "^1.1.0",
    "babyjubjub-ecdsa": "^1.2.0",
    "bip-schnorr": "^0.6.7",
    "blakejs": "^1.2.1",
    "bls-signatures": "^2.0.3",
    "chalk": "^4.1.2",
