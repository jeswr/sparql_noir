{
  "name": "ts",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "build:signature": "cd noir/bin/signature && nargo build",
    "build:tsc": "tsc",
    "build:noir": "node dist/scripts/setup.js",
    "build:transform": "cargo build --manifest-path transform/Cargo.toml --release",
    "build:wasm": "cd transform && wasm-pack build --target nodejs --release",
    "build": "npm run build:tsc && npm run build:noir && npm run build:signature && npm run build:wasm",
    "setup": "node dist/scripts/setup.js",
    "sign": "node dist/scripts/sign.js",
    "transform": "cargo run --manifest-path transform/Cargo.toml --",
    "prove": "node dist/scripts/prove.js",
    "verify": "node dist/scripts/verify.js",
    "e2e": "node dist/scripts/e2e.js",
    "e2e:verbose": "node dist/scripts/e2e.js -v",
    "test": "npm run build:tsc && npm run e2e",
    "test:sparql": "npx tsx test/run-sparql-tests.ts",
    "test:sparql:features": "npx tsx test/run-sparql-tests.ts --features",
    "test:sparql:bgp": "npx tsx test/run-sparql-tests.ts -t bgp",
    "test:sparql:summary": "npx tsx test/run-sparql-tests.ts -o summary",
    "test:transform": "cargo test --manifest-path transform/Cargo.toml",
    "test:snapshot": "npx tsx test/run-snapshot-tests.ts",
    "test:snapshot:update": "npx tsx test/run-snapshot-tests.ts --update --ts-only",
    "test:circuits:generate": "npx tsx test/generate-circuit-tests.ts",
    "test:circuits:generate:full": "npx tsx test/generate-circuit-tests.ts --full --force",
    "test:circuits:list": "npx tsx test/generate-circuit-tests.ts --list",
    "test:circuits:dry-run": "npx tsx test/generate-circuit-tests.ts --dry-run",
    "test:circuits:run": "npx tsx test/run-circuit-tests.ts",
    "test:circuits:compile": "npx tsx test/run-circuit-tests.ts --compile-only",
    "test:parsing": "npx tsx test/test-sparql-parsing.ts",
    "example:sign": "node dist/scripts/sign.js -i inputs/data/ -o temp/",
    "example:verify": "node dist/scripts/verify.js -i temp/data.json --signature-only",
    "example": "npm run example:sign && npm run example:verify",
    "build:noir:gen": "cd transform && cargo run -p transform -- --input ../inputs/data/data.ttl --output ../temp/data.json --query ../inputs/sparql.rq",
    "benchmark": "npx tsx src/scripts/benchmarks/noir-benchmark.ts benchmark",
    "benchmark:signature": "npx tsx src/scripts/benchmarks/noir-benchmark.ts benchmark noir/bin/signature",
    "benchmark:encode": "npx tsx src/scripts/benchmarks/noir-benchmark.ts benchmark noir/bin/encode",
    "benchmark:verify": "npx tsx src/scripts/benchmarks/noir-benchmark.ts benchmark noir/bin/verify_inclusion",
    "benchmark:all": "npx tsx src/scripts/benchmarks/noir-benchmark.ts benchmark",
    "benchmark:list": "npx tsx src/scripts/benchmarks/noir-benchmark.ts list-circuits",
    "benchmark:help": "npx tsx src/scripts/benchmarks/noir-benchmark.ts --help",
    "test:sparql10": "node ts.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aztec/bb.js": "^1.2.1",
    "@aztec/foundation": "^2.1.7",
    "@noir-lang/noir_js": "^1.0.0-beta.16",
    "@noir-lang/noir_wasm": "^1.0.0-beta.16",
    "@noir-lang/noirc_abi": "^1.0.0-beta.15",
    "@zk-kit/baby-jubjub": "^1.0.3",
    "@zk-kit/eddsa-poseidon": "^1.1.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "n3": "^1.26.0",
    "rdf-dereference-store": "^1.4.0",
    "rdf-string-ttl": "^2.0.1",
    "rdfjs-c14n": "^3.1.3",
    "secp256k1": "^5.0.1",
    "secp256r1": "^0.0.3",
    "sparqlalgebrajs": "^5.0.2"
  },
  "devDependencies": {
    "@rdfjs/types": "^2.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/secp256k1": "^4.0.7",
    "rdf-test-suite": "^2.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
